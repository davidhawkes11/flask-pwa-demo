{
  "name": "Flask PWA",
  "dockerFile": "Dockerfile",
  "context": "..",
  
  "remoteEnv": {
  "FLASK_APP": "app.py",
  "PYTHONUNBUFFERED": "1"
}

  "settings": {
    "terminal.integrated.shell.linux": "/bin/bash"
  },
  "extensions": [
    "ms-python.python",
    "ms-python.vscode-pylance"
  ],
  "postCreateCommand": "pip install -r requirements.txt"
}
